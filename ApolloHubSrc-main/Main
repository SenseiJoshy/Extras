nouni=false
local function lo(l)
loadstring(game:HttpGet(l,true))()
nouni=true
end
if game.PlaceId==1962086868 then --Tower of hell
lo("https://raw.githubusercontent.com/ApolloHub/AutoLoader/main/TOH")
end

if game.PlaceId==2248408710 then --Destruction Simulator
lo("https://raw.githubusercontent.com/ApolloHub/AutoLoader/main/DestructionSim")
end

if game.PlaceId==142823291 then --Murder Mystery 2
lo("https://raw.githubusercontent.com/ApolloHub/AutoLoader/main/mm2.txt")
end

if game.PlaceId==286090429 then --Arsenal
lo("https://raw.githubusercontent.com/ApolloHub/AutoLoader/main/Arsenal")
end

if game.PlaceId==292439477 then --Phantom Forces
lo("https://raw.githubusercontent.com/ApolloHub/AutoLoader/main/PhantomForces")
end

if game.PlaceId==155615604 then --Prison Life
lo("https://raw.githubusercontent.com/ApolloHub/AutoLoader/main/PrisonLife")
end

if game.PlaceId==1537690962 then --Bee Swarm Sim
lo("https://raw.githubusercontent.com/ApolloHub/AutoLoader/main/BeeSwarmSim")
end

if game.PlaceId==3527629287 then --Big PaintBall
