local CoreGui = game:GetService("StarterGui")

   CoreGui:SetCore("SendNotification", {
	Title = "Game Detected!";
	Text = "Loading UI | Bee Swarm Simulator";
	Duration = 5;
})

wait(5) 

local Library = loadstring(game:HttpGet("https://pastebin.com/raw/6W1ZqV53"))()

local a = Library:Window("Apollo Hub")
local auto = Library:Window("Auto-Farm")
local b = Library:Window("Credits")
local CoreGui = game:GetService("StarterGui")

   CoreGui:SetCore("SendNotification", {
	Title = "Loaded!";
	Text = "Made by APollo#2391";
	Duration = 5;
})




--A

a:Toggle("Collect All Tokens", function(t)
    if t then
        getgenv().collecttoken = true 
        print("on")
    else
        getgenv().collecttoken = false 
    end   
end)

a:Toggle("Collect Snow Flakes", function(t)
        if t then
        getgenv().snwoflakefarm = true 
        print("on")
    else
        getgenv().snwoflakefarm = false 
    end
end)

a:Toggle("Auto Clicker", function(t)
        if t then
        wait(5)
        getgenv().autoclicker = true

    else
        getgenv().autoclicker = false 
    end
end)

a:Toggle("Bob Auto Farm", function(t)
        if t then
        getgenv().mobautofarm = true
    else
        getgenv().mobautofarm = false
    end
end)

