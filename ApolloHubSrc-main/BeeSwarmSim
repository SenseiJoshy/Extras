    local args = {
        [1] = 4
    }
    game:GetService("ReplicatedStorage").Events.ClaimHive:FireServer(unpack(args))
    local args = {
        [1] = 5
    }
    game:GetService("ReplicatedStorage").Events.ClaimHive:FireServer(unpack(args))
    local args = {
        [1] = 6
    }
    game:GetService("ReplicatedStorage").Events.ClaimHive:FireServer(unpack(args))
